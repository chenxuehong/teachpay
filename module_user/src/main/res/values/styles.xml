<resources>


    <style name="ll_chat_group_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_marginLeft">@dimen/dp_12</item>
        <item name="android:layout_marginRight">@dimen/dp_12</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_height">44dp</item>
    </style>
    <style name="tv_left_chat_group_style">
        <item name="android:textColor">@color/color_333333</item>
        <item name="android:textSize">@dimen/sp_16</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_height">wrap_content</item>
    </style>


    <style name="style_tv_color_333333_sp_16">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/color_333333</item>
        <item name="android:textSize">@dimen/sp_16</item>
    </style>

    <style name="MyCheckBox">

        <item name="colorControlActivated">@color/mainColor</item>
    </style>
</resources>
<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/item_note_img_fl"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="91dp">

    <com.makeramen.roundedimageview.RoundedImageView
        app:riv_corner_radius="@dimen/dp_5"
        app:riv_border_width="0.5dp"
        app:riv_border_color="@color/color_666666"
        android:scaleType="fitXY"
        android:id="@+id/item_note_img_icon"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <ImageView
        android:id="@+id/item_note_img_icon_delete"
        android:layout_gravity="right|top"
        android:scaleType="fitXY"
        android:src="@mipmap/remove_pic"
        android:layout_width="@dimen/dp_20"
        android:layout_height="@dimen/dp_20"/>
</FrameLayout>
<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MyVideoPlayer">
        <attr name="autoPlay" format="boolean" />
    </declare-styleable>

    <declare-styleable name="empty">
        <attr name="img" format="reference" />
        <attr name="msg" format="string" />
        <attr name="msg2" format="string" />
        <attr name="loading" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CourseTableLayoutStyle">
        <attr name="rowNumber" format="integer" />
        <attr name="columnNumber" format="integer" />
        <attr name="courseArrangeName" format="string" />
    </declare-styleable>

    <declare-styleable name="customer_title">
        <attr name="leftImg" format="reference" />
        <attr name="leftVisible" format="boolean" />
        <attr name="leftText" format="string" />
        <attr name="leftTextColor" format="color" />
        <attr name="titleTextColor" format="color" />
        <attr name="titleTextSize" format="dimension" />
        <attr name="rightVisible" format="boolean" />
        <attr name="title" format="string" />
        <attr name="backgroundColor" format="color" />
        <attr name="rightText" format="string" />
        <attr name="rightTextColor" format="color" />
        <attr name="rightImg" format="reference" />
        <attr name="rightImg2" format="reference" />
        <attr name="rightImg3" format="reference" />
        <attr name="showStateBar" format="boolean" />
        <attr name="showLine" format="boolean" />
        <attr name="statusBarColor" format="color" />
    </declare-styleable>
    <declare-styleable name="common_title">
        <attr name="myleftImg" format="reference" />
        <attr name="myleftImgVisible" format="boolean" />

        <attr name="myleftText" format="string" />
        <attr name="myleftTextColor" format="color" />

        <attr name="mytitle" format="string" />
        <attr name="mytitleTextColor" format="color" />
        <attr name="mytitleTextSize" format="dimension" />

        <attr name="mybackgroundColor" format="color" />

        <attr name="myrightImg1" format="reference" />
        <attr name="myrightImg2" format="reference" />
        <attr name="myrightImg3" format="reference" />


        <attr name="myrightText" format="string" />
        <attr name="myrightTextColor" format="color" />

        <attr name="myshowStateBar" format="boolean" />
        <attr name="mystatusBarColor" format="color" />

        <attr name="myshowLine" format="boolean" />
        <attr name="mylineColor" format="color" />
    </declare-styleable>
    <item name="xbanner_pointId" type="id"/>
    <declare-styleable name="XBanner">
        <attr name="isAutoPlay" format="boolean" />
        <attr name="isHandLoop" format="boolean" />
        <attr name="AutoPlayTime" format="integer|reference" />
        <attr name="pointNormal" format="reference" />
        <attr name="pointSelect" format="reference" />
        <attr name="indicatorDrawable" format="reference" />
        <attr name="pointsVisibility" format="boolean" />
        <attr name="isTipsMarquee" format="boolean" />
        <attr name="pointsPosition" format="enum">
            <enum name="LEFT" value="0" />
            <enum name="CENTER" value="1" />
            <enum name="RIGHT" value="2" />
        </attr>
        <attr name="pointsContainerBackground" format="reference|color" />
        <attr name="pointContainerPosition" format="enum">
            <enum name="TOP" value="10" />
            <enum name="BOTTOM" value="12" />
        </attr>
        <!-- 指示点容器左右内间距 -->
        <attr name="pointContainerLeftRightPadding" format="dimension" />
        <!-- 指示点上下内间距 -->
        <attr name="pointTopBottomPadding" format="dimension" />
        <!-- 指示点左右内间距 -->
        <attr name="pointLeftRightPadding" format="dimension" />
        <!-- 提示文案的文字颜色 -->
        <attr name="tipTextColor" format="reference|color" />
        <!-- 提示文案的文字大小 -->
        <attr name="tipTextSize" format="dimension" />
        <!--是否是数字指示器-->
        <attr name="isShowNumberIndicator" format="boolean" />
        <!-- 数字指示器背景 -->
        <attr name="numberIndicatorBacgroud" format="reference" />
        <!-- 当只有一页数据时是否显示指示器，默认值为false -->
        <attr name="isShowIndicatorOnlyOne" format="boolean" />
        <!-- 图片切换速度 -->
        <attr name="pageChangeDuration" format="integer" />
        <!--图片轮播框架展占位图-->
        <attr name="placeholderDrawable" format="reference" />
        <!--是否开启一屏显示多个模式 默认不开启-->
        <attr name="isClipChildrenMode" format="boolean" />
        <!--一屏显示多个左右间距-->
        <attr name="clipChildrenLeftRightMargin" format="dimension" />
        <!--一屏显示多个上下间距-->
        <attr name="clipChildrenTopBottomMargin" format="dimension" />
        <!--设置viewpager间距-->
        <attr name="viewpagerMargin" format="dimension" />
        <!--少于三张是否支持一屏多显模式-->
        <attr name="isClipChildrenModeLessThree" format="boolean" />
        <!--是否展示提示文案-->
        <attr name="isShowTips" format="boolean" />
        <!--banner轮播区域底部margin-->
        <attr name="bannerBottomMargin" format="dimension" />
        <!--设置viewPager ClipChildren-->
        <attr name="viewPagerClipChildren" format="boolean" />
        <!-- 占位图缩放模式 -->
        <attr name="android:scaleType" />
    </declare-styleable>

    <declare-styleable name="RatioLayout">
        <attr name="radio2" format="float" />
    </declare-styleable>

    <declare-styleable name="BounceScrollView">
        <attr name="is_width_fix_drawable_size_ratio" format="boolean" />
        <attr name="is_height_fix_drawable_size_ratio" format="boolean" />
        <attr name="height_to_width_ratio" format="float" />
        <attr name="width_to_width_ratio" format="float" />
    </declare-styleable>

    <declare-styleable name="AutoFitImageView">
        <attr name="autoFitWidth" format="boolean" />

    </declare-styleable>

    <declare-styleable name="StateView">
        <attr name="bindId" format="reference" />
    </declare-styleable>
    <declare-styleable name="ShadowLayout">
        <attr name="sl_cornerRadius" format="dimension" />
        <attr name="sl_shadowRadius" format="dimension" />
        <attr name="sl_shadowColor" format="color" />
        <attr name="sl_dx" format="dimension" />
        <attr name="sl_dy" format="dimension" />
    </declare-styleable>
    <declare-styleable name="IndexBar">
        <attr name="indexBarTextSize" />
        <attr name="indexBarPressBackground" format="color" />
    </declare-styleable>
</resources>